height = int(input().strip())
age = int(input().strip())

if height < 120:
    height_status = "Not Allowed"
elif height <= 140:
    height_status = "Must ride with Adult"
else:
    height_status = "Can ride alone"

if age < 5:
    age_status = "Not Allowed"
elif age <= 12:
    age_status = "Must ride with Adult"
else:
    age_status = "Can ride alone"

if "Not Allowed" in (height_status, age_status):
    print("Not Allowed")
elif "Must ride with Adult" in (height_status, age_status):
    print("Must ride with Adult")
    print("200")
else:
    print("Can ride alone")
    print("300")
